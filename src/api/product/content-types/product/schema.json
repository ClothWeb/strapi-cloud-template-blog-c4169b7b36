{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "transatel_id": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "product_name": {
      "type": "string"
    },
    "transatel_price": {
      "type": "decimal"
    },
    "wondersim_price": {
      "type": "decimal"
    },
    "validity": {
      "type": "string"
    },
    "coverage": {
      "type": "string"
    },
    "tags": {
      "type": "json"
    },
    "countries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::country.country",
      "mappedBy": "product"
    },
    "data": {
      "type": "integer"
    },
    "code": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::code.code",
      "inversedBy": "products"
    }
  }
}
