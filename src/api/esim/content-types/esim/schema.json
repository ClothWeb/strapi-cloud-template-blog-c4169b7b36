{
  "kind": "collectionType",
  "collectionName": "esims",
  "info": {
    "singularName": "esim",
    "pluralName": "esims",
    "displayName": "Esim",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "serial": {
      "type": "string",
      "unique": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
